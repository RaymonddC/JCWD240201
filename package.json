{
  "name": "final-project-template",
  "scripts": {
    "start": "lerna run start --stream",
    "build": "lerna run build --stream",
    "serve": "lerna run serve --stream",
    "clean": "lerna clean --y",
    "test": "lerna run test",
    "install:server": "lerna add --scope=server",
    "install:client": "lerna add --scope=client",
    "install:server:dev": "lerna add --scope=server --dev",
    "install:client:dev": "lerna add --scope=client --dev",
    "server": "lerna exec --scope server yarn",
    "client": "lerna exec --scope client yarn"
  },
  "private": true,
  "devDependencies": {
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "lerna": "^5.6.2",
    "prettier": "^3.0.0"
  },
  "workspaces": [
    "projects/*"
  ],
  "dependencies": {
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "dayjs": "^1.11.9",
    "exceljs": "^4.3.0",
    "firebase": "^10.2.0",
    "formik": "^2.4.2",
    "maplibre-gl": "^3.3.0",
    "react-draft-wysiwyg": "^1.15.0",
    "react-dropdown-select": "^4.10.0",
    "react-icons": "^4.10.1",
    "react-map-gl": "^7.1.4",
    "react-to-print": "^2.14.13",
    "react-zoom-pan-pinch": "^3.1.0",
    "recharts": "^2.7.3"
  }
}
